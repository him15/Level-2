class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        if(head == null){
            return null;
        }
        ListNode slow = head;
        ListNode fast = head;
        
        while(n-->0){
            fast = fast.next;
        }
        
        if(fast == null){
            ListNode temp = head;
            head = head.next;
            temp.next = null;
            return head;
        }
        
        while(fast.next != null){
            fast = fast.next;
            slow = slow.next;
        }
        slow.next = slow.next.next;
        return head;
    }
}
